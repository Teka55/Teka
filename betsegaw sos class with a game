В В В В В В В В В В В В playSound: function(type) {
В В В В В В В В В В В В В В В В if(this.audioCtx.state === 'suspended') this.audioCtx.resume();
В В В В В В В В В В В В В В В В const osc = this.audioCtx.createOscillator();
