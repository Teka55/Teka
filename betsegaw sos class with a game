В В В В В В В В В В В В В В В В osc.connect(gain); gain.connect(this.audioCtx.destination);

Teka Tesfaye, [1/14/2026 10:05 PM]
В В В В В В В В В В В В В В В В 
В В В В В В В В В В В В В В В В if (type === 'correct') {
В В В В В В В В В В В В В В В В В В В В osc.type = 'sine'; osc.frequency.setValueAtTime(600, this.audioCtx.currentTime);
В В В В В В В В В В В В В В В В В В В В osc.frequency.exponentialRampToValueAtTime(1200, this.audioCtx.currentTime + 0.1);
В В В В В В В В В В В В В В В В } else if (type === 'wrong') {
В В В В В В В В В В В В В В В В В В В В osc.type = 'sawtooth'; osc.frequency.setValueAtTime(150, this.audioCtx.currentTime);
В В В В В В В В В В В В В В В В В В В В osc.frequency.exponentialRampToValueAtTime(50, this.audioCtx.currentTime + 0.3);
В В В В В В В В В В В В В В В В } else if (type === 'click') {
В В В В В В В В В В В В В В В В В В В В osc.type = 'triangle'; osc.frequency.setValueAtTime(800, this.audioCtx.currentTime);
