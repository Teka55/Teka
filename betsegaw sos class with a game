В В В В В В В В В В В В prepGame: function(mode) {
В В В В В В В В В В В В В В В В if(this.lives <= 0) { this.resetGame(); return; }
В В В В В В В В В В В В В В В В this.currentMode = mode;
В В В В В В В В В В В В В В В В this.showScreen('game-screen');
В В В В В В В В В В В В В В В В this.els.input.value = ''; this.els.input.focus();
В В В В В В В В В В В В В В В В this.nextQuestion();
В В В В В В В В В В В В },
В В В В В В В В В В В В nextQuestion: function() {
В В В В В В В В В В В В В В В В if(this.lives <= 0) { this.gameOver(); return; }
В В В В В В В В В В В В В В В В this.els.input.value = ''; this.els.input.focus();
В В В В В В В В В В В В В В В В this.startTimer(15);
В В В В В В В В В В В В В В В В if (this.currentMode === 'math') this.generateMath();
В В В В В В В В В В В В В В В В else this.generateEnglish();
В В В В В В В В В В В В },
В В В В В В В В В В В В r: function(min, max) { return Math.floor(Math.random() * (max - min + 1)) + min; },
В В В В В В В В В В В В generateMath: function() {
В В В В В В В В В В В В В В В В const diff = this.scoreStreak > 5 ? 2 : (this.scoreStreak > 2 ? 1 : 0);
В В В В В В В В В В В В В В В В let n1, n2, op, ans;
В В В В В В В В В В В В В В В В if(diff === 0) { n1=this.r(1,10); n2=this.r(1,10); op = Math.random()>.5?'+':'-'; }
В В В В В В В В В В В В В В В В else { n1=this.r(10,50); n2=this.r(2,10); op = Math.random()>.5?'+':'*'; }
В В В В В В В В В В В В В В В В if(op === '+') ans = n1+n2;
В В В В В В В В В В В В В В В В if(op === '-') { if(n1<n2) [n1,n2]=[n2,n1]; ans = n1-n2; }
В В В В В В В В В В В В В В В В if(op === '*') ans = n1*n2;
